---
const { title, url } = Astro.props;
---
<style>

    .slide {
        margin-left: 25%;
        margin: 15%;
        height: 100px;
        width: 200px;
        background-color: #fff;
        text-align: center;
    }

    .title{
        text-decoration: none;
        color: black;
    }

</style>

<li>
    <!--Number -->
    <div class="slide">
        <a class="title" href={url}>{title}</a>
    </div>
</li>

